public class EinsPunktZwei {
    public static void main(String[] args) {
        System.out.println("Hello world!"); // an dieser Zeile ist eine freundliches Programm zu erkennen

        double z = Math.random();   // 0 <= z < 1
        int zufallszahl;
        int i;
        int anzahl_zahlen_kleiner_als_550=0;
        int anzahl_zahlen_groesser_gleich_als_550=0;
        for(i = 100; i>0;i--) {
            zufallszahl = (int) (Math.random() * 900 + 100);
            System.out.println(zufallszahl);
            if (zufallszahl < 550)
                anzahl_zahlen_kleiner_als_550++;
            else
                anzahl_zahlen_groesser_gleich_als_550++;
        }
        System.out.print("% der Zahlen Kleiner:");
        System.out.println(anzahl_zahlen_kleiner_als_550);
        System.out.print("% der Zahlen Größer:");
        System.out.println(anzahl_zahlen_groesser_gleich_als_550);
    }
}
